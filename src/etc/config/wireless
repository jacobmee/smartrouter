
config wifi-device 'radio0'
	option type 'mac80211'
	option macaddr '5c:63:bf:fb:71:14'
	list ht_capab 'LDPC'
	list ht_capab 'SHORT-GI-20'
	list ht_capab 'SHORT-GI-40'
	list ht_capab 'TX-STBC'
	list ht_capab 'RX-STBC1'
	list ht_capab 'DSSS_CCK-40'
	option noscan '1'
	option country 'US'
	option hwmode '11g'
	option htmode 'HT40'
	option txpower '20'
	option log_level '3'
	option channel '6'

config wifi-device 'radio1'
	option type 'mac80211'
	option macaddr '5c:63:bf:fb:71:15'
	list ht_capab 'LDPC'
	list ht_capab 'SHORT-GI-20'
	list ht_capab 'SHORT-GI-40'
	list ht_capab 'TX-STBC'
	list ht_capab 'RX-STBC1'
	list ht_capab 'DSSS_CCK-40'
	option noscan '1'
	option country 'US'
	option hwmode '11a'
	option channel '153'
	option htmode 'HT40'
	option txpower '17'
	option log_level '3'

config wifi-iface
	option device 'radio0'
	option mode 'ap'
	option ssid 'MiTang'
	option network 'lan'
	option encryption 'psk2'
	option key 'abc123_+U'

config wifi-iface
	option device 'radio1'
	option mode 'ap'
	option ssid 'MiTang.5G'
	option network 'lan'
	option encryption 'psk2'
	option key 'abc123_+'
	option hidden '1'

